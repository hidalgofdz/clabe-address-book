    "check-format": "npm run prettier -- --list-different \"**/*\""
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.5",
    "@commitlint/config-conventional": "^17.6.5",
    "prettier": "2.8.8"
  }
}
